public class String{

public static void main(String[] args) {
String s="aish"	;
String t = "isha";

char a1[] = s.toCharArray();
char a2[] = t.toCharArray();
Arrays.sort(a1);
Arrays.sort(a2);
String s1 = new String(a1);
String s2 = new String(a2);
if(s1.equals(s2))
System.out.println("true");
else
System.out.println("false");

}}
